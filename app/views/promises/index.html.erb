<h1>Promises#index</h1>
<p>Find me in app/views/promises/index.html.erb</p>

<ul>
    <% @promises.each do |promise| %>
      <% if promise.text == 'Other'%>
        <li><%= promise.other_text %></li>
      <% else %>
        <li><%= promise.text %></li>
      <% end %>
    <% end %>
</ul>

<%= link_to 'create a new promise', new_promise_path %>
