<h1 class="red-text text-center">Hi <%= current_user.firstname %>!</h1>
<h2 class="red-text text-center">Here are your promises:</h1>

<ul>
    <% @promises.each do |promise| %>
    <div class="outer">
    <li><%= promise.text %></li>
    </div>
    <% end %>
</ul>

<%= link_to 'create a new promise', new_promise_path %>

